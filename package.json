{
  "name": "react-bluekit",
  "version": "0.3.1",
  "description": "Visualisation and Playground generated from Components",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "check": "npm run lint && npm run test",
    "clean": "rimraf lib",
    "lint": "eslint src test examples",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run check",
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueberryapps/react-bluekit.git"
  },
  "keywords": [
    "es6",
    "es7",
    "babel",
    "react-component",
    "visualization",
    "playground",
    "development",
    "ui-ki",
    "ui",
    "design",
    "node"
  ],
  "author": "Ondrej Bartas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blueberryapps/react-bluekit/issues"
  },
  "homepage": "https://github.com/blueberryapps/react-bluekit#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": ">=1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "2.0.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-react": "^3.5.1",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.12.2",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.15.0",
    "mocha": "^2.3.3",
    "react-hot-loader": "^1.3.1",
    "react-svg-icon-generator": "^0.3.0",
    "rimraf": "^2.4.3",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": ">=0.13",
    "react-dom": ">=0.13"
  },
  "dependencies": {
    "color": ">=0.10",
    "fuzzy": ">=0.1",
    "gulp": ">=3",
    "gulp-eslint": "^3.0.1",
    "immutable": ">=3",
    "lodash": "^4.16.6",
    "nunjucks": "^3.0.0",
    "radium": ">=0.15",
    "react-ace": ">=3",
    "react-color": "^2.11.1",
    "react-copy-to-clipboard": ">=4",
    "react-docgen": "^2.13.0",
    "react-highlight": ">=0.5",
    "react-pure-render": ">=1",
    "react-responsive": "^1.2.6",
    "tosource": ">=1"
  },
  "ava": {
    "files": [
      "src/**/__test__/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  }
}
